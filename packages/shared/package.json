{
  "name": "@coh/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean --tsconfig tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "format": "prettier -w ."
  },
  "dependencies": {
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "@types/node": "^22.10.2"
  }
}
