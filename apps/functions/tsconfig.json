{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "bundler",
        "outDir": "dist",
        "rootDir": "src",
        "target": "ES2022",
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strict": false,
        "types": [
            "node"
        ]
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}